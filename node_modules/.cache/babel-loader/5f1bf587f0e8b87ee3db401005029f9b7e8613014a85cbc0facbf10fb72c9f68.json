{"ast":null,"code":"import CharacterCard from './CharacterCard.vue';\nexport default {\n  props: ['characters', 'searchQuery'],\n  components: {\n    CharacterCard\n  },\n  computed: {\n    filteredCharacters() {\n      if (this.searchQuery) {\n        return this.characters.filter(character => character.name.toLowerCase().includes(this.searchQuery.toLowerCase()));\n      } else {\n        return this.characters;\n      }\n    },\n    changePage(page) {\n      this.page = page <= 0 || page > this.pages ? this.page : page;\n      this.fetch();\n    }\n  }\n};","map":{"version":3,"names":["CharacterCard","props","components","computed","filteredCharacters","searchQuery","characters","filter","character","name","toLowerCase","includes","changePage","page","pages","fetch"],"sources":["/Users/danielhorne/Desktop/Ditto/rickandmorty/src/components/SearchResults.vue"],"sourcesContent":["<template>\n    <div class=\"containr\">\n        <h2>Search Results \n            <!-- {{ filteredCharacters.length }} out of {{ characters.length }} -->\n        </h2>\n        <!-- <li \n            v-for=\"character in filteredCharacters\" \n            :key=\"character.id\"\n        >\n            <h2>{{ character.name }}</h2>\n            <img :src=\"character.image\" :alt=\"character.name\" />\n            <p>Status: {{ character.status }}</p>\n            <p>Species: {{ character.species }}</p>\n        </li> -->\n\n        <div class=\"search-results\">\n            <CharacterCard\n                v-for=\"character in filteredCharacters\"\n                :key=\"character.id\"\n                :character=\"character\"\n            />\n        </div>\n\n\n\n      <nav class=\"pagination\" role=\"navigation\" aria-label=\"pagination\">\n        <a class=\"pagination-previous\" v-on:click=\"changePage( page - 1)\">Anterior</a>\n        <ul class=\"pagination-list\">\n          <li>\n            <a class=\"pagination-link is-current\">{{page}}</a>\n          </li>\n        </ul>\n        <a class=\"pagination-next\" v-on:click=\"changePage( page + 1)\">Siguiente</a>\n      </nav>\n\n    </div>\n</template>\n  \n<script>\nimport CharacterCard from './CharacterCard.vue';\n\nexport default {\n    props: ['characters', 'searchQuery'],\n    components: {\n        CharacterCard\n    },\n    computed: {\n        filteredCharacters() {\n            if (this.searchQuery) {\n            return this.characters.filter(character =>\n                character.name.toLowerCase().includes(this.searchQuery.toLowerCase())\n            );\n            } else {\n                return this.characters;\n            }\n        },\n\n    changePage(page) {\n      this.page = page <= 0 || page > this.pages ? this.page : page;\n      this.fetch();\n    },\n    }\n};\n</script>\n  \n  <!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped lang=\"scss\">\n\nh2 {\n    margin-bottom: 1.5rem;\n}\n.search-results {\n    display: flex;\n    flex-wrap: wrap;\n    justify-content: center;\n    gap: 1rem;\n}\n</style>\n  "],"mappings":"AAuCA,OAAOA,aAAY,MAAO,qBAAqB;AAE/C,eAAe;EACXC,KAAK,EAAE,CAAC,YAAY,EAAE,aAAa,CAAC;EACpCC,UAAU,EAAE;IACRF;EACJ,CAAC;EACDG,QAAQ,EAAE;IACNC,kBAAkBA,CAAA,EAAG;MACjB,IAAI,IAAI,CAACC,WAAW,EAAE;QACtB,OAAO,IAAI,CAACC,UAAU,CAACC,MAAM,CAACC,SAAQ,IAClCA,SAAS,CAACC,IAAI,CAACC,WAAW,CAAC,CAAC,CAACC,QAAQ,CAAC,IAAI,CAACN,WAAW,CAACK,WAAW,CAAC,CAAC,CACxE,CAAC;MACD,OAAO;QACH,OAAO,IAAI,CAACJ,UAAU;MAC1B;IACJ,CAAC;IAELM,UAAUA,CAACC,IAAI,EAAE;MACf,IAAI,CAACA,IAAG,GAAIA,IAAG,IAAK,KAAKA,IAAG,GAAI,IAAI,CAACC,KAAI,GAAI,IAAI,CAACD,IAAG,GAAIA,IAAI;MAC7D,IAAI,CAACE,KAAK,CAAC,CAAC;IACd;EACA;AACJ,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}