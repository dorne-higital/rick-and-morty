{"ast":null,"code":"import axios from \"axios\";\nimport Search from './components/Search.vue';\nimport SearchResults from './components/SearchResults.vue';\nimport Modal from './Modal.vue';\nexport default {\n  name: 'App',\n  components: {\n    Search,\n    SearchResults,\n    Modal\n  },\n  data: function () {\n    return {\n      characters: [],\n      page: 1,\n      pages: 1,\n      search: \"\",\n      modalVisible: false,\n      currentCharacter: {}\n    };\n  },\n  created() {\n    this.fetch();\n  },\n  methods: {\n    fetch() {\n      const params = {\n        page: this.page,\n        name: this.search\n      };\n      let url = \"https://rickandmortyapi.com/api/character\";\n      let result = axios.get(url, {\n        params\n      }).then(res => {\n        this.characters = res.data.results;\n        this.pages = res.data.info.pages;\n        console.log(this.characters);\n      }).catch(console.log);\n    },\n    changePage(page) {\n      this.page = page <= 0 || page > this.pages ? this.page : page;\n      this.fetch();\n    },\n    searchData(query) {\n      this.search = query;\n      this.page = 1; // Reset page to 1 when performing a new search\n      this.fetch();\n    },\n    showModal(character) {\n      this.selectedCharacter = character;\n      this.modalVisible = true;\n    },\n    closeModal() {\n      this.modalVisible = false;\n      this.selectedCharacter = {};\n    },\n    async fetchOne(id) {\n      let result = await axios.get(`https://rickandmortyapi.com/api/character/${id}/`);\n      this.currentCharacter = result.data;\n      this.modal = true;\n    }\n  }\n};","map":{"version":3,"names":["axios","Search","SearchResults","Modal","name","components","data","characters","page","pages","search","modalVisible","currentCharacter","created","fetch","methods","params","url","result","get","then","res","results","info","console","log","catch","changePage","searchData","query","showModal","character","selectedCharacter","closeModal","fetchOne","id","modal"],"sources":["/Users/danielhorne/Desktop/Ditto/rickandmorty/src/App.vue"],"sourcesContent":["<template>\n    <div class=\"container\">\n        <h1>Rick & Morty</h1>\n        <Search @search=\"searchData\" />\n        <SearchResults\n            :characters=\"characters\"\n            :searchQuery=\"search\"\n        />\n\n        <Modal \n            v-if=\"modalVisible\" \n            :character=\"selectedCharacter\" \n            @close-modal=\"closeModal\" \n        />\n\n\n\n        <nav class=\"pagination\" role=\"navigation\" aria-label=\"pagination\">\n            <a class=\"pagination-previous\" v-on:click=\"changePage( page - 1)\">Prev</a>\n            <ul class=\"pagination-list\">\n                <li>\n                    <a class=\"pagination-link is-current\">{{page}}</a>\n                </li>\n            </ul>\n            <a class=\"pagination-next\" v-on:click=\"changePage( page + 1)\">Next</a>\n        </nav>\n    </div>\n</template>\n  \n<script>\n    import axios from \"axios\";\n    import Search from './components/Search.vue';\n    import SearchResults from './components/SearchResults.vue';\n    import Modal from './Modal.vue';\n  \n  export default {\n    name: 'App',\n    components: {\n      Search,\n      SearchResults,\n      Modal\n    },\n    data: function() {\n      return {\n        characters: [],\n        page: 1,\n        pages: 1,\n        search: \"\",\n        modalVisible: false,\n        currentCharacter: {}\n      };\n    },\n    created() {\n      this.fetch();\n    },\n    methods: {\n      fetch() {\n        const params = {\n          page: this.page,\n          name: this.search\n        };\n  \n        let url = \"https://rickandmortyapi.com/api/character\";\n        let result = axios\n            .get(url, { params })\n            .then(res => {\n              this.characters = res.data.results;\n              this.pages = res.data.info.pages;\n              console.log(this.characters);\n            })\n            .catch(console.log);\n      },\n      changePage(page) {\n        this.page = page <= 0 || page > this.pages ? this.page : page;\n        this.fetch();\n      },\n      searchData(query) {\n        this.search = query;\n        this.page = 1; // Reset page to 1 when performing a new search\n        this.fetch();\n      },\n        showModal(character) {\n            this.selectedCharacter = character;\n            this.modalVisible = true;\n        },\n        closeModal() {\n            this.modalVisible = false;\n            this.selectedCharacter = {};\n        },\n      async fetchOne(id) {\n        let result = await axios.get(\n            `https://rickandmortyapi.com/api/character/${id}/`\n        );\n        this.currentCharacter = result.data;\n        this.modal = true;\n      }\n    }\n  };\n</script>\n  \n\n<style lang=\"scss\">\n\n*,\n*::before,\n*::after {\n    box-sizing: border-box;\n    font-family: Avenir, Helvetica, Arial, sans-serif;\n    scroll-behavior: smooth;\n}\n\nbody,\nh1,\nh2,\nh3,\nh4,\np {\n    margin: 0;\n}\n\nul[role='list'],\nol[role='list'] {\n    list-style: none;\n}\n\nbody {\n    background: #3a4767;\n    color: #ebebee;\n    width: 100%;\n}\n\n.container {\n    padding-top: 3rem;\n    text-align: center;\n}\n</style>\n"],"mappings":"AA8BI,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,MAAK,MAAO,yBAAyB;AAC5C,OAAOC,aAAY,MAAO,gCAAgC;AAC1D,OAAOC,KAAI,MAAO,aAAa;AAEjC,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,UAAU,EAAE;IACVJ,MAAM;IACNC,aAAa;IACbC;EACF,CAAC;EACDG,IAAI,EAAE,SAAAA,CAAA,EAAW;IACf,OAAO;MACLC,UAAU,EAAE,EAAE;MACdC,IAAI,EAAE,CAAC;MACPC,KAAK,EAAE,CAAC;MACRC,MAAM,EAAE,EAAE;MACVC,YAAY,EAAE,KAAK;MACnBC,gBAAgB,EAAE,CAAC;IACrB,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,KAAK,CAAC,CAAC;EACd,CAAC;EACDC,OAAO,EAAE;IACPD,KAAKA,CAAA,EAAG;MACN,MAAME,MAAK,GAAI;QACbR,IAAI,EAAE,IAAI,CAACA,IAAI;QACfJ,IAAI,EAAE,IAAI,CAACM;MACb,CAAC;MAED,IAAIO,GAAE,GAAI,2CAA2C;MACrD,IAAIC,MAAK,GAAIlB,KAAI,CACZmB,GAAG,CAACF,GAAG,EAAE;QAAED;MAAO,CAAC,EACnBI,